//cnt=cnt*2;

for(int i = 1;i&lt;=cnt;i++)
{
if(!(get(Fields.In,"SRC_"+i).getString(r).equals(get(Fields.In,"TGT_"+i).getString(r))))
{
		if(flag == 0)
